{
  "name": "user-pool",
  "version": "1.0.0",
  "description": "Simple user pool service",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "rm -rf dist && tsc && tsc-alias",
    "start": "npm run build && node dist/index.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "jest --runInBand ./tests/integration/*",
    "test:integration:watch": "jest --watch --runInBand ./tests/integration/*",
    "test:unit": "jest ./tests/unit/*",
    "test:unit:watch": "jest --watch ./tests/unit/*"
  },
  "keywords": [
    "user-pool",
    "service"
  ],
  "author": "Lucas Machado Rodrigues",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/supertest": "^2.0.12",
    "jest": "^28.1.3",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.6.11"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.5.1"
  }
}
